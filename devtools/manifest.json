{
  "name": "DevTools",
  "version": "1.0",
  "description": "Anything to do with dev tools.",
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "http://json2csharp.com/Home/GenerateClasses"
  ],
  "browser_action": {
    "default_title": "Devtools 1.0",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://maxcdn.bootstrapcdn.com; object-src 'self'"
}
